source RFID {
  proto MQTT
  path 104.198.225.118@@reader_to_reader_agent
}

source Another {
  proto HTTP
  path /arm
  method POST
}

service Arm_01 {
  proto MQTT
  path 104.198.225.118@@arm_01_agent_to_arm_01
}

service Verify {
  proto HTTP
  path localhost:3000/verify
  method GET
}

Another ~> Arm_01
RFID ~> Arm_01
RFID ~> Verify