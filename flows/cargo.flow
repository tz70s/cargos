# renew syntax (0.1.4)

source rfid {
  proto mqtt
  path 104.198.225.118@@reader_to_reader_agent
}

sink classifier {
  proto http
  path cargo-cls.default.svc.cluster.local/tag
  method post
}

rfid ~> classifier

source classifier {
  proto http
  path /classify
  method post
}

sink shelf {
  proto http
  path cargo-shelf.default.svc.cluster.local/cls
  method post
}

sink arm01 {
  proto mqtt
  path 104.198.225.118@@arm_01_agent_to_arm_01
}

classifier ~> shelf
classifier ~> arm01

source arm01 {
  proto mqtt
  path 104.198.225.118@@arm_01_to_arm_01_agent
}

source shelf {
  proto http
  path /transportation
  method post
}

sink agv01 {
  proto mqtt
  path 104.198.225.118@@agv_01_agent_to_agv_01
}

shelf ~> agv01
arm01 ~> agv01

source agv01 {
  proto mqtt
  path 104.198.225.118@@agv_01_to_agv_01_agent 
}

sink arm02 {
  proto mqtt
  path 104.198.225.118@@arm_02_agent_to_arm_02
}

agv01 ~> arm02
