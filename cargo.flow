source RFID {
  proto MQTT
  path tag
}

service Classifier {
  proto HTTP
  path /api/tag
  method POST
}

RFID ~> Classifier

source Classifier {
  proto HTTP
  path /classify
  method GET
}

service Shelf {
  proto HTTP
  path /api/cls
  method POST
}

service Arm {
  proto MQTT
  path classify
}

Classifier ~> Shelf
Classifier ~> Arm
